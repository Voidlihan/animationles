<Window x:Class="AnimationLesson.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnimationLesson"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas x:Name="canvas">
        <Rectangle x:Name="loadRectangle" HorizontalAlignment="Left" Height="20" Stroke="Black" VerticalAlignment="Top" Width="30" Fill="Green"/>
        <Button Name="loadButton" Content="Загрузить" HorizontalAlignment="Center" Margin="0,102,0,0" VerticalAlignment="Top" Click="Load" Width="100"/>
        <ProgressBar HorizontalAlignment="Center" Height="25" Margin="0,144,0,0" VerticalAlignment="Top" Width="100"/>

    </Canvas>
</Window>
